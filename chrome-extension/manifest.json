{
  "name": "pageNote",
  "version": "0.0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons":{
    "16":"./icons/16.png",
    "48":"./icons/48.png",
    "128":"./icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/saltfish666/pageNote-node?token=*"],
      "js": ["./content_scripts/login.js"]
    },{
      "matches": ["<all_urls>"],
      "js": ["./content_scripts/getToken.js"]
    }, {
      "matches": ["<all_urls>"],
      "js": ["./dist/bundle.js"]
    }
  ],
  "background": {
    "scripts": [
      "./background/controlToken.js"
    ]
  }
}
