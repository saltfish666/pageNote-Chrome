{
  "name": "pageNote",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/saltfish666/pageNote-node?token=*"],
      "js": ["./content_scripts/login.js"]
    },{
      "matches": ["<all_urls>"],
      "js": ["./content_scripts/getToken.js"]
    }, {
      "matches": ["<all_urls>"],
      "js": ["./dist/bundle.js"]
    }
  ],
  "background": {
    "scripts": [
      "./background/controlToken.js","./background/bg1.js"
    ]
  }
}
